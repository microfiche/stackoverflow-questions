                <script>
                    function initAISearchAlphaBanner() {
                        const $signupLink = $('.js-ai-search-alpha-banner-link');
                        const $dismissBtn = $('.js-ai-search-alpha-banner .s-popover--close');

                        $signupLink.on('click', function () {
                            dismissAISearchAlphaBanner();
                        });

                        $dismissBtn.on('click', function () {
                            dismissAISearchAlphaBanner();
                        });
                    }

                    function dismissAISearchAlphaBanner() {
                        $('.js-ai-search-alpha-banner').remove();
                        $.cookie('notice-aap', '1', { path: '/', expires: 30 });

                        if (!StackExchange.options.user.isAnonymous) {
                            $.post("/ai-search-alpha-launch-items/dismiss", {
                                fkey: StackExchange.options.user.fkey,
                                itemToDismiss: "PopoverBanner"
                            });
                        }
                    }

                    initAISearchAlphaBanner();
                </script>


        
