        -webkit-backface-visibility: hidden;
        -webkit-transform: translate3d(0,0,0);
        visibility: visible;
    }

    @media (max-width: 640px) {
        .theme-selector {
            background-size: 393px, 750px;
        }
    }

    .theme-selection {
        animation: selector-animation ease-out 3s;
    }

    .theme-btn:hover {
        background-color: #000;
        box-shadow: 0px 1px 0px #424242;
    }

    .theme-selected {
        background-color: #1d1d1d;
        box-shadow: 0px 1px 0px #424242, inset 0px 1px 0px #000000;
    }

    .theme-title {
        animation: title-animation ease-out 2s;
    }

    @keyframes background-animation {
        0%   { background-position-y: -200px, 50px; }
        100% { background-position-y: 0, 0; }
    }

    @keyframes title-animation {
        0% {
            opacity: 0;
            transform: scale(4);
        }
        100% {
            opacity: 1;
            transform: scale(1);
        }
    }

    @keyframes selector-animation {
        0% { opacity:0; }
        60% { opacity:0; }
        100% { opacity:1; }
    }
</style>


