# main.py
logger = logging.getLogger('...
                </div>
            <div class="s-post-summary--meta">
                <div class="s-post-summary--meta-tags d-inline-block tags js-tags t-python t-logging">

<ul class='ml0 list-ls-none js-post-tag-list-wrapper d-inline'><li class='d-inline mr4 js-post-tag-list-item'><a href="/questions/tagged/python" class="s-tag post-tag flex--item mt0 js-tagname-python" title="show questions tagged &#39;python&#39;" aria-label="show questions tagged &#39;python&#39;" rel="tag" aria-labelledby="tag-python-tooltip-container" data-tag-menu-origin="Unknown" itemprop="keywords">python</a></li><li class='d-inline mr4 js-post-tag-list-item'><a href="/questions/tagged/logging" class="s-tag post-tag flex--item mt0 js-tagname-logging" title="show questions tagged &#39;logging&#39;" aria-label="show questions tagged &#39;logging&#39;" rel="tag" aria-labelledby="tag-logging-tooltip-container" data-tag-menu-origin="Unknown" itemprop="keywords">logging</a></li></ul>
