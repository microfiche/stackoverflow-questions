                `timescale 1ns/1ps

module write_8bits(
  output reg OLED_D1
);
integer i;
reg [0:7] data=8'b10010111;
reg clk;

always@(posedge clk) begin
  for (i=0;i&lt;8;i=i+1)begin
      OLED_D1&lt;=data[i];//...
