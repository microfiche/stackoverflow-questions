                int[] arr = {1, 2, 3, 4, 5};
for (int i = 0; i &lt; arr.length / 2; i++) {
    int temp = arr[i];
    arr[i] = arr[arr.length - 1 - i];
    arr[arr.length - 1 - i] = temp;
} 

