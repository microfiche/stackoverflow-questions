        <li class="s-sidebarwidget--item d-flex px16">
            <div class="flex--item1 fl-shrink0">
<div class="favicon favicon-stackoverflowmeta" title="Meta Stack Overflow"></div>            </div>
            <div class="flex--item wmn0 ow-break-word">
                <a href="https://meta.stackoverflow.com/questions/434267/2025-community-moderator-election-results" class="js-gps-track" data-ga="[&quot;community bulletin board&quot;,&quot;Featured on Meta&quot;,&quot;https://meta.stackoverflow.com/questions/434267/2025-community-moderator-election-results&quot;,null,null]" data-gps-track="communitybulletin.click({ priority: 6, position: 6, location: questionspage })">2025 Community Moderator Election Results</a>
            </div>
        </li>
        </ul>
    </div>



				
