        </h3>
            <div class="s-post-summary--content-excerpt">
                template &lt;typename T&gt;
class SNode{
    private:
    T data;
    SNode&lt;T&gt; *next;
    
    public:
    SNode() : data(NULL), next(NULL){};
    SNode(T data) : data(data), next(NULL){};

};

