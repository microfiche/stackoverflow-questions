                            <button class="s-btn s-btn__sm s-btn__filled flex--item" type="submit" data-se-uql-target="applyButton">Apply filter</button>
                        </div>
                        <div class="flex--item">
                            <button class="s-btn s-btn__sm" data-action="se-uql#cancelEditor" type="button">Cancel</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>

</div>



