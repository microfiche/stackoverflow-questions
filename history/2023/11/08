    opt.omni = 'BwoLCIagvdDohLA8EAUgAigBOgBIAD6XEKIBAIJaMe8';
    opt.refresh = !1;
    opt.refreshInterval = 90;
    opt.sf = !0;
    opt.hb = !1;
    opt.ll = !0;
